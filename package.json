{
  "name": "@wootapa/object-evaluator-ol",
  "version": "1.0.17",
  "description": "Extension of @wootapa/object-evaluator with spatial operators for openlayers and ogc query output.",
  "main": "dist/waoe.ol.umd.js",
  "types": "dist/waoe.ol.d.ts",
  "module": "dist/waoe.ol.esm.js",
  "browser": "dist/waoe.ol.umd.js",
  "scripts": {
    "start": "npm run build",
    "lint": "eslint --fix --ext .ts ./src && eslint --fix --ext .ts ./test",
    "test": "mocha -r performance-now -r esm -r jsdom-global/register -r ts-node/register test/*.spec.ts",
    "build": "npm run lint && rollup -c"
  },
  "files": [
    "dist/**/*.{js,map,ts}"
  ],
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wootapa/wa-object-evaluator"
  },
  "keywords": [
    "WootApa",
    "Evaluate",
    "Logical",
    "Comparison",
    "Openlayers",
    "OGC",
    "CQL",
    "GIS",
    "Spatial"
  ],
  "author": "Andreas Petersson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wootapa/wa-object-evaluator/issues"
  },
  "homepage": "https://github.com/wootapa/wa-object-evaluator/tree/ol#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@types/chai": "^4.2.11",
    "@types/jsdom": "^16.2.3",
    "@types/mocha": "^8.0.0",
    "@types/ol": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-typescript": "^3.0.0",
    "esm": "^3.2.25",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.0.1",
    "ol": "^6.2.1",
    "rollup": "^2.0.6",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-node": "^8.10.1",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "ol": ">= 6.0.0"
  },
  "dependencies": {
    "@turf/boolean-contains": "^6.0.1",
    "@turf/boolean-intersects": "^6.0.2",
    "performance-now": "^2.1.0"
  }
}
